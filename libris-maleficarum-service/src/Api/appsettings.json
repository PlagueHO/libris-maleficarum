      "Continent": { "MinVersion": 1, "MaxVersion": 1 },
      "Country": { "MinVersion": 1, "MaxVersion": 1 },
      "Region": { "MinVersion": 1, "MaxVersion": 1 },
      "City": { "MinVersion": 1, "MaxVersion": 1 },
      "Building": { "MinVersion": 1, "MaxVersion": 1 },
      "Room": { "MinVersion": 1, "MaxVersion": 1 },
      "Faction": { "MinVersion": 1, "MaxVersion": 1 },
      "Event": { "MinVersion": 1, "MaxVersion": 1 },
      "Quest": { "MinVersion": 1, "MaxVersion": 1 },
      "Item": { "MinVersion": 1, "MaxVersion": 1 },
      "Session": { "MinVersion": 1, "MaxVersion": 1 },
      "Folder": { "MinVersion": 1, "MaxVersion": 1 },
      "GeographicRegion": { "MinVersion": 1, "MaxVersion": 1 },
      "PoliticalRegion": { "MinVersion": 1, "MaxVersion": 1 },
      "CulturalRegion": { "MinVersion": 1, "MaxVersion": 1 },
